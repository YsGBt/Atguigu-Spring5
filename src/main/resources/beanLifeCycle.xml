<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="order" class="com.atguigu.spring5.demo.Order" init-method="initMethod" destroy-method="destroyMethod">
    <property name="orderName" value="手机"></property>
  </bean>

  <!-- 配置后置处理器 -->
  <bean id="beanPost" class="com.atguigu.spring5.demo.MyBeanPost"></bean>


</beans>